to launch 
	do "swipeup" 
	if ImageFound(text:"Belk Dev", waitFor:10,caseSensitive:yes)
		tap FoundImageLocation()
	else
		do "SwipeLeft "
		tap text:"Belk Dev",waitfor:45,caseSensitive:yes
	end if
	
end launch




to Login username,password

	tap image:"LoginButton",waitfor:20,SearchRectangle:screen_part_handler.ScreenPart("BottomRightQuadrant")
	set the  SearchRectangle to screen_part_handler.ScreenPart("HorizontalMiddle")	
	if ImageFound(text:"Skip", waitFor:10)
		tap FoundImageLocation()
	end if
	tap text:"Create Account",hotspot:[0,-100]
	//tap text:"Sign In",waitfor:40,caseSensitive:yes
	tap text:"Email",waitfor:20,hotspot:[10,35]
	typetext username
	tap text:"Password",waitfor:20,hotspot:[10,25]
	typetext password
	set the nextKeyDelay to 0.2
	typetext tabkey,tabkey,return
	set the SearchRectangle to screen_part_handler.ScreenPart("BottomHalf")
	if ImageFound(text:"Turn on Location Services", waitFor:50)
		tap text:"Not Now",waitfor:70,caseSensitive:yes
	else
		log "Location services pop up not rised"
	end if
	set the SearchRectangle to []

end Login




to  GuidedSearching ProductCategory,subcategory,subcategory1
	if ImageFound(text:"Skip", waitFor:10,caseSensitive:yes,SearchRectangle:screen_part_handler.ScreenPart("HorizontalMiddle"))
		tap FoundImageLocation()
	end if
	tap image:"shop",caseSensitive:yes,waitfor:60,SearchRectangle:screen_part_handler.ScreenPart("BottomLeftQuadrant")
	waitfor 100,text:"What can we help you find?",caseSensitive:yes,textDifference:"1",SearchRectangle:screen_part_handler.ScreenPart("TopHalf")
	moveto FoundImageLocation()
	wait 2
	Repeat until ImageFound(text:ProductCategory, waitFor:10,caseSensitive:yes)
		set the swipeSpeed to 80
		swipeup
		wait 1
	End repeat
	
	tap FoundImageLocation()
	wait 3
	Repeat until ImageFound(text:subcategory, waitFor:10)
		set the swipeSpeed to 80
		swipeup
		wait 1
	End repeat
	
	tap FoundImageLocation()
	if ImageFound(text:subcategory1, waitFor:10)
		tap FoundImageLocation()
	else
		log "didn't asked for inner category"
	end if
	set the SearchRectangle to []
end GuidedSearching



to  GuidedProduct ProductName1,size1
	
	Repeat until ImageFound(text:ProductName1,caseSensitive:yes,textDifference:"3", waitFor:10,ignoreSpaces:yes)
		set the swipeSpeed to 120
		Swipeup 
		wait 2
	End repeat
	tap FoundImageLocation()
	
	set the SearchRectangle to screen_part_handler.ScreenPart("BottomHalf")
	waitfor 100,text:"Add to Bag"
	tap text:"Select Size",caseSensitive:yes
	waitfor 100,text:"Select a Size",SearchRectangle:screen_part_handler.ScreenPart("VerticalMiddle")
	tap text:size1,caseSensitive:yes,SearchRectangle:screen_part_handler.ScreenPart("LeftHalf")
	wait 3
	tap text:"Add to Bag",waitfor:15,caseSensitive:yes
	waitfor 100,text:"You May Also Like",caseSensitive:yes
	wait 2
	tap text:"View Bag",waitfor:30,caseSensitive:yes,SearchRectangle:screen_part_handler.ScreenPart("HorizontalMiddle")
	waitfor 50,text:"Proceed To Checkout",caseSensitive:yes
	set the SearchRectangle to []
end GuidedProduct


