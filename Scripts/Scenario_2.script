to  Checkout  ZIPCode,ProductName3
	set the SearchRectangle to screen_part_handler.ScreenPart("BottomHalf")
	tap image:"LoginButton",waitfor:20
	tap FoundImageLocation()
	tap text:"Favorites",caseSensitive:yes,SearchRectangle:screen_part_handler.ScreenPart("HorizontalMiddle")
	tap text:"Add to Bag",SearchRectangle:screen_part_handler.ScreenPart("TopHalf")
	waitfor 100,text:"You May Also Like",caseSensitive:yes
	wait 2
	set the SearchRectangle to screen_part_handler.ScreenPart("HorizontalMiddle")
	tap text:"View Bag",waitfor:30,caseSensitive:yes
	
	tap text:"Shipping",waitfor:20,caseSensitive:yes
	wait 3
	set the swipeSpeed to 110
	do "swipeup"
	wait 3
	tap text:"Free Pickup",waitfor:25,caseSensitive:yes
	set the SearchRectangle to screen_part_handler.ScreenPart("TopHalf")
	wait 3
	waitfor 50,text:"ZIP Code",caseSensitive:yes
	tap FoundImageLocation()
	typetext ZIPCode,return
	if ImageFound(text:"Pick Up Here", waitFor:30,caseSensitive:yes,SearchRectangle:screen_part_handler.ScreenPart("BottomHalf"))
		tap FoundImageLocation()
	else
		log "Zip code entered is invalid/ not deliverable to that zip code"
		tap text:"Cancel",waitfor:30,caseSensitive:yes
	end if
	
	waitfor 100,text:"Edit Bag",caseSensitive:yes
	tap FoundImageLocation()
	waitfor 100,text:"Shipping"
	set the SearchRectangle to empty
	
	put ImageRectangle(text:ProductName3, waitFor:20,textDifference:"2") into rect
	tap text:"Remove",caseSensitive:yes,waitfor:15,SearchRectangle:rect+[0,-120,170,170]
	wait 3
	if ImageFound(imageName:"plusQty", waitFor:15)
		tap FoundImageLocation()
	else
		log "Quantity increse button is disabled"
	end if
	tap text:"Done",waitfor:25,SearchRectangle:screen_part_handler.ScreenPart("TopRightQuadrant")
	set the SearchRectangle to screen_part_handler.ScreenPart("HorizontalMiddle")
	if ImageFound(text:":Qty: 2", waitFor:15,ignoreSpaces:yes,textDifference:"1")
		log "Quantity of the product increased"
	else
		log "Quantity button is disabled"
	end if
	
	
	Repeat until  ImageFound(text:"Coupon Code", waitFor:7)
		set swipeSpeed to 100
		swipeup
		wait 3
	End repeat
	tap FoundImageLocation()
	wait 2
	tap text:"Enter Coupon Code",waitfor:30,caseSensitive:yes,SearchRectangle:screen_part_handler.ScreenPart("BottomHalf")
	typetext "EXTRAEXTRA"
	wait 2
	typetext tabkey,return
	set the SearchRectangle to screen_part_handler.ScreenPart("HorizontalMiddle")
	if ImageFound(text:"Invalid coupon code", waitFor:15)
		tap text:"OK",caseSensitive:yes,waitfor:20
		log "Coupon code entered is invalid"
	else if ImageFound(text:"Applied", waitFor:5)
		log "Coupon applied"
	end if
	tap text:"Add New Coupon",caseSensitive:yes,waitfor:50,hotspot:[0,-200]
	
	tap text:"Proceed To Checkout",caseSensitive:yes,waitfor:20,SearchRectangle:screen_part_handler.ScreenPart("BottomHalf")
	Repeat until ImageFound(text:"Place Order", waitFor:7,caseSensitive:yes)
		SwipeUp 
		wait 2
	End repeat
	tap text:"Security Code",caseSensitive:yes,waitfor:15,hotspot:[20,50]
	typetext "123"
	tap text:"Place Order",waitfor:15,caseSensitive:yes
	(*tap text:"Place Order",waitfor:20,caseSensitive:yes
	tap text:"CVV Code",caseSensitive:yes,waitfor:10
	typetext "123"
	wait 2
	tap text:"Place Order",waitfor:20,caseSensitive:yes*)
	set the SearchRectangle to screen_part_handler.ScreenPart("TopHalf")
	if ImageFound(text:"Thank you for your order", waitFor:25)
		log "order is placed"
	else
		log "order is not placed"
	end if
	put ImageRectangle(text:"Thank you for your order", waitFor:15) into rect
	put readtext(rect+[390,0,350,0]) into OrderNumber
	//tap text:"Continue Shopping",waitfor:15,caseSensitive:yes,SearchRectangle:screen_part_handler.ScreenPart("BottomHalf")
	tap ImageFound(imageName:"Cross", waitFor:15)
	tap image:"LoginButton",waitfor:15,SearchRectangle:screen_part_handler.ScreenPart("BottomHalf")
	tap FoundImageLocation()
	tap text:"My Orders",caseSensitive:yes,waitfor:20
	if ImageFound(text:"Order Number:"&&OrderNumber, waitFor:15)
		log "Order history is verified"
	else
		log "order history is not verified"
	end if
end Checkout