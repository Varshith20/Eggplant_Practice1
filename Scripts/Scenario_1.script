to PlaceOrder
	
	
	if ImageFound(text:"No, thanks", waitFor:15,textDifference:"2",ignoreSpaces:yes,SearchRectangle:screen_part_handler.ScreenPart("BottomHalf"))
		click FoundImageLocation()
	else
		log "Card Data saving pop up not found"
	end if
	set the SearchRectangle to screen_part_handler.ScreenPart("TopHalf")
	waitfor 100,text:"Return to Shopping Bag",caseSensitive:yes
	Repeat until  ImageFound(text:"Place Order", waitFor:10,caseSensitive:yes)
		set the swipeSpeed to 80
		swipeup
		wait 4
	End repeat
	tap FoundImageLocation()
	if ImageFound(text:"Thank you for your order", waitFor:25)
		log "order is placed"
	else
		log "order is not placed"
	end if
	set the SearchRectangle to []
end PlaceOrder







